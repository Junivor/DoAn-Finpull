{
  "title": "FinPull Consumer & Pipeline",
  "panels": [
    {
      "type": "gauge",
      "title": "Consumer Queue Fullness",
      "targets": [{"expr": "finpull_kafka_consumer_queue_fullness"}]
    },
    {
      "type": "graph",
      "title": "Consumer Queue Depth",
      "targets": [{"expr": "finpull_kafka_consumer_queue_depth"}]
    },
    {
      "type": "heatmap",
      "title": "Consumer Handle Latency",
      "targets": [{"expr": "sum(rate(finpull_kafka_consumer_handle_seconds_bucket[5m])) by (le, topic)"}]
    },
    {
      "type": "graph",
      "title": "Pipeline Throttle Count",
      "targets": [{"expr": "increase(pipeline_throttle_count[5m])"}]
    },
    {
      "type": "graph",
      "title": "Pipeline Buffer Depth",
      "targets": [{"expr": "pipeline_buffer_depth"}]
    }
  ]
}