# Development environment example - DO NOT commit real secrets
# Copy this file to `config.dev.env.local` or `config.dev.env` and add secret values locally.

ENV=development
APP_NAME=finpull
LOG_LEVEL=debug

# Finnhub
FINNHUB_API_KEY=<replace_with_your_finnhub_api_key>
SYMBOLS=BINANCE:BTCUSDT,OANDA:XAU_USD,OANDA:WTICO_USD,OANDA:XAG_USD

# Backend
BACKEND=kafka
ENABLE_METRICS=true
METRICS_PORT=2112

# Kafka
KAFKA_BROKERS=localhost:9092
KAFKA_TOPIC=finnhub

# ClickHouse
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=9000
CLICKHOUSE_HTTP_PORT=8123
CLICKHOUSE_USER=finpull-clickhouse
# CLICKHOUSE_PASSWORD=your_clickhouse_password_here
CLICKHOUSE_DATABASE=finpull_dev

# Postgres
POSTGRES_USER=finpull
# POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DB=finpull
POSTGRES_PORT=5433
POSTGRES_HOST=localhost

# Superset
SUPERSET_SECRET_KEY=dev_secret_key_not_for_production
SUPERSET_ADMIN_USERNAME=admin
# SUPERSET_ADMIN_PASSWORD=your_superset_admin_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
# GRAFANA_ADMIN_PASSWORD=your_grafana_admin_password

# Notes:
# - Keep actual secrets out of version control. Use k8s Secrets, environment variables or a secret manager.

